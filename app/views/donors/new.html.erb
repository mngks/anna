<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8">
      <p>We require more information to verify your account before you start sharing food</p>

      <%= simple_form_for @donor do |f| %>
        <%= f.input :full_name %>
        <%= f.input :contact_number%>
        <%= f.button :submit, class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
