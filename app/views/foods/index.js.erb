var tab_content = document.querySelector('.tab-content');
tab_content.innerHTML = '';

function createNewCard(innerHTML = '') {
  tab_content.insertAdjacentHTML('beforeend', innerHTML);
}

<% if @foods.any? %>
  <% @foods.each do |food| %>
    createNewCard('<%= j render "card", user: @user %>');
  <% end %>
<% end %>
